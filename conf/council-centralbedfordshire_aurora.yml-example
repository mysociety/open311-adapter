oauth:
  username: username
  password: password
  client_id: client_id
  client_secret: secret
  access_token_url: url

# A map from service_code to:
#
#   name: The display name for the category
#   group: Optional category group
#   parameters: dictionary of default parameters to use
category_mapping:
  categoryA:
    name: "Category A"
    group: "Group 1"
    parameters:
      caseTypeCode: Code
  categoryB:
    name: "Category B"
    parameters:
      caseTypeCode: Code
